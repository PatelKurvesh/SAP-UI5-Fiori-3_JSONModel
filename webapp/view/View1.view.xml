<mvc:View controllerName="GithubSAP-UI5-Fiori-3_JSONModel.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<Label text="Name"></Label>
					<Input value="{/student/name}"></Input>
					<Label text="Enrollment No."></Label>
					<Input value="{/student/no}"></Input>
					<Label text="Semester"></Label>
					<Input value="{/student/sem}"></Input>
					<Label text="Age"></Label>
					<Input value="{studAge>/student/sAge}"></Input>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>